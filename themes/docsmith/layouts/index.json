{{- $.Scratch.Add "index" slice -}}
{{- range where .Site.Pages "Type" "not in"  (slice "page" "json") -}}
{{- $.Scratch.Add "index" (dict "title" .Title "ref" .RelPermalink "keywords" .Params.keywords "deprecated" .Params.deprecated "shortguide" .Params.shortguide "toc" (.TableOfContents | plainify )) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
